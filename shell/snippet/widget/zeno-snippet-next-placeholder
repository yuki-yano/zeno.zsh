#autoload

local out=$(echo "$BUFFER" | zeno --mode=next-placeholder)

if [[ $(echo "$out" | head -1) != "success" ]]; then
  return
fi

local buffer=$(echo "$out" | head -2 | tail -1)
local cursor=$(echo "$out" | head -3 | tail -1)

if [[ $buffer != "" ]]; then
  BUFFER=$buffer
  CURSOR=$cursor
fi
